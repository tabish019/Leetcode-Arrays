class Solution {
    public int maxProfit(int[] prices) {
        int n = prices.length;
        int buy = Integer.MAX_VALUE;
        int max_sell = 0;
        for(int i=0;i<n;i++){
            buy = Math.min(buy,prices[i]);
            max_sell = Math.max(max_sell,prices[i]-buy);
        }
        return max_sell;
}
}
