class Solution {
    public int findMaxConsecutiveOnes(int[] nums) {
        int count = 0;
        int temp = 0;
        for(int i=0;i<nums.length;i++){
         // temp =  Math.max(temp,count);
            if(nums[i] == 1){
                count++;  
                temp = Math.max(temp,count); 
            }
           else{
                count = 0;
            } 
        }
        return temp;
        
    }
}
